{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/index.js"
        }
    ],
    "env": {
        "MONGO_URI": "@mongo_uri",
        "JWT_SECRET": "@jwt_secret"
    }
}